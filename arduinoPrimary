  #define SCREEN_WIDTH 128 // setting the display width, in pixels
  #define SCREEN_HEIGHT 32 // setting the display height, in pixels

  #define OLED_RESET  4// Reset pin # (or -1 if sharing Arduino reset pin)
  #define SCREEN_ADDRESS 0x3C //defining the address of the LCD
  Adafruit_SSD1306 display(OLED_RESET);



void setup() {
  // put your setup code here, to run once:

  Wire.begin(); //Starting the wire library 

  
